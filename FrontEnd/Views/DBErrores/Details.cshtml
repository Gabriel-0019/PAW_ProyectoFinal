@model FrontEnd.Models.DBErroresViewModel

@{
    ViewData["Title"] = "Details";
}

<h1>Detalle del error</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdError)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdError)
        </dd>
        <dt class = "col-sm-2">
            Número del error
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ErrorNumero)
        </dd>
        <dt class = "col-sm-2">
            Estatus del error
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ErrorStatus)
        </dd>
